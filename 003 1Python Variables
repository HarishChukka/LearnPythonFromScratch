print("Creating Variables")

x = 5
y = "John"
x="6"
print(x)
print(y)

x = 4       # x is of type int
x = "Sally" # x is now of type str
print(x)

print("Casting")
x = str(3)    # x will be '3'
y = int(3)    # y will be 3
z = float(3)  # z will be 3.0
print(x)
print(y)
print(z)


myvar = "myvar"
print("myvar")

my_var = "John"
print("my_var")

_my_var = "_my_var"
print("_my_var")

myVar = "myVar"
print("myVar")

MYVAR = "MYVAR"
print("MYVAR")

myvar2 = "myvar2"
print("myvar2")